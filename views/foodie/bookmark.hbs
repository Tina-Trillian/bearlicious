<h1>My Bookmarks</h1>
<div class="bg-white">
  {{#each bookmarks}}
  <div class="restaurant-card">
    <h3><a href="/restaurant/{{this._id}}">{{this.name}}</a></h3>
    <div class="restaurant-image">
      <img src="{{this.picPath}}" alt="">
    </div>
    <div class="restaurant-info">
      {{this.description}}
    </div>
  {{/each}}
  </div>
</div>